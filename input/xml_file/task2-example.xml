<?xml version="1.0" encoding="UTF-8"?>
<MolSim>
    <simulation_parameters>
        <end_time>20</end_time>
        <delta_time>0.0005</delta_time>
        <output_basename>../output/MD_vtk</output_basename>
        <write_frequency>10</write_frequency>
        <r_cutoff_radius>3.0</r_cutoff_radius>
        <domain_size>
            <x>180.0</x>
            <y>90.0</y>
            <z>0.0</z>
        </domain_size>
    </simulation_parameters>

     <boundary_conditions>
        <left>Outflow</left>
        <right>Outflow</right>
        <top>Outflow</top>
        <bottom>Outflow</bottom>
    </boundary_conditions>

    <cuboids>
        <cuboid>
            <coordinate>
                <x>20.0</x>
                <y>20.0</y>
                <z>0.0</z>
            </coordinate>
            <dimensions>
                <x>100</x>
                <y>20</y>
                <z>1</z>
            </dimensions>
            <mesh_width>1.1225</mesh_width>
            <mass>1.0</mass>
            <initial_velocity>
                <x>0.0</x>
                <y>0.0</y>
                <z>0.0</z>
            </initial_velocity>
            <average_velocity>0.0</average_velocity>
        </cuboid>

        <cuboid>
            <coordinate>
                <x>70.0</x>
                <y>60.0</y>
                <z>0.0</z>
            </coordinate>
            <dimensions>
                <x>20</x>
                <y>20</y>
                <z>1</z>
            </dimensions>
            <mesh_width>1.1225</mesh_width>
            <mass>1.0</mass>
            <initial_velocity>
                <x>0.0</x>
                <y>-10.0</y>
                <z>0.0</z>
            </initial_velocity>
            <average_velocity>0.0</average_velocity>
        </cuboid>
    </cuboids>
</MolSim>
