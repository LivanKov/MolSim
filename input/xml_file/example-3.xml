<?xml version="1.0" encoding="UTF-8"?>
<MolSim>
    <simulation_parameters>
        <end_time>10</end_time>
        <delta_time>0.005</delta_time>
        <output_basename>../output</output_basename>
        <write_frequency>10</write_frequency>
        <r_cutoff_radius>3</r_cutoff_radius>
        <domain_size>
            <x>243.0</x>
            <y>180.0</y>
            <z>0.0</z>
        </domain_size>
    </simulation_parameters>

    <cuboids>
        <cuboid>
            <coordinate>
                <x>1.5</x>
                <y>2.0</y>
                <z>0.0</z>
            </coordinate>
            <dimensions>
                <x>200</x>
                <y>20</y>
                <z>1</z>
            </dimensions>
            <mesh_width>1.2</mesh_width>
            <mass>1.0</mass>
            <initial_velocity>
                <x>0.0</x>
                <y>0.0</y>
                <z>0.0</z>
            </initial_velocity>
            <average_velocity>0.0</average_velocity>
        </cuboid>
    </cuboids>
</MolSim>